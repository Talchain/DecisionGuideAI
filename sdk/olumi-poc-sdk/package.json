{
  "name": "olumi-poc-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Olumi Scenario Sandbox PoC",
  "type": "module",
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && echo 'SDK built successfully'",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "olumi",
    "scenario",
    "sandbox",
    "decision",
    "analysis",
    "typescript",
    "sdk"
  ],
  "author": "Olumi",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olumi/scenario-sandbox"
  }
}