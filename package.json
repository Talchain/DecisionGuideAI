{
  "name": "decision-mind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:connection": "node --loader ts-node/esm src/lib/test-connection.ts",
    "test": "vitest run --reporter=verbose",
    "e2e": "playwright test",
    "typecheck": "tsc -p tsconfig.ci.json --noEmit",
    "poc:start": "./tools/poc-start.sh",
    "poc:stop": "docker compose --profile poc down",
    "poc:logs": "docker compose --profile poc logs -f",
    "poc:status": "docker compose --profile poc ps",
    "integration:check": "tsx integration/integration-check.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@floating-ui/react": "^0.26.9",
    "@supabase/supabase-js": "^2.39.7",
    "date-fns": "^3.3.1",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@testing-library/react": "^14.3.1",
    "@testing-library/dom": "^9.3.1",
    "jsdom": "^24.0.0",
    "react-is": "^18.3.1",
    "vitest": "^1.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "@playwright/test": "^1.47.2"
  }
}